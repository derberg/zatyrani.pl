import PanelContent from "../../../components/niebocross/PanelContent.astro";
import { t as tBase } from "../../../utils/i18n";

const t = (key: string) => tBase(key, 'en');
---

<Layout 
  title={`NieboCross - ${t("niebocross.dashboard.title")}`}
