{
  "name": "zatyrani-website",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run og:niebocross && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "build:scripts:facebook": "tsc --project ./tsconfig.scripts.json",
    "fetch:facebook": "npm run build:scripts:facebook && node ./dist/facebook.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "member:list": "node scripts/postgress/member-list.js",
    "member:add": "node scripts/postgress/member-add.js",
    "og:niebocross": "tsx scripts/generate-niebocross-og.tsx",
    "member:remove": "node scripts/postgress/member-remove.js"
  },
  "lint-staged": {
    "*.{js,ts,astro}": [
      "eslint --fix",
      "git add",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.1.0",
    "@octokit/plugin-create-or-update-text-file": "^5.1.0",
    "@octokit/rest": "^21.0.2",
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.81.1",
    "@vercel/node": "^5.0.1",
    "astro": "^4.15.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "twilio": "^4.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@fontsource/inter": "^5.2.8",
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "^22.10.2",
    "@typescript-eslint/parser": "^8.46.3",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "react": "^19.2.3",
    "satori": "^0.18.3",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.2"
  }
